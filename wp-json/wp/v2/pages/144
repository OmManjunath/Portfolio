 }\n} \n\n#right-left {\n  cursor: pointer; \/* Changes the cursor to a pointer when hovered *\/\n}\n\n<\/style>\n\n\n<script>\n\ndocument.addEventListener(\"DOMContentLoaded\", function () {\n  const sliderContainer = document.querySelector(\".slider-container\");\n  const sliderTrack = document.querySelector(\".slider-track\");\n  const sliderItems = Array.from(document.querySelectorAll(\".slider-item\"));\n  const totalItems = sliderItems.length;\n  let currentIndex = totalItems; \/\/ Start from first clone\n  let autoSlideInterval;\n\n  \/* Clone slider items for seamless looping *\/\n  sliderItems.forEach(item => {\n    sliderTrack.appendChild(item.cloneNode(true)); \/\/ Clone at end\n    sliderTrack.insertBefore(item.cloneNode(true), sliderTrack.firstChild); \/\/ Clone at start\n  });\n\n  \/* Function to determine items per view based on window width *\/\n  function getItemsPerView() {\n    if (window.innerWidth <= 750) return 1;\n    if (window.innerWidth <= 768) return 2;\n    if (window.innerWidth <= 1024) return 3;\n    return 4; \/\/ Default to 4 items per view for larger screens\n  }\n\n  \/* Function to update slider position *\/\n  function updateSlider() {\n    const itemsPerView = getItemsPerView();\n    const translateX = -(currentIndex * 100) \/ itemsPerView;\n    sliderTrack.style.transform = `translateX(${translateX}%)`;\n\n    \/* Seamless looping reset *\/\n    sliderTrack.style.transition = 'transform 0.5s ease'; \/\/ Smooth transition\n\n    if (currentIndex >= totalItems * 2) {\n      \/* If reached the end of clones, reset to the original first item *\/\n      setTimeout(() => {\n        sliderTrack.style.transition = 'none'; \/\/ Remove transition temporarily\n        currentIndex = totalItems; \/\/ Reset to the first cloned item\n        const resetTranslateX = -(currentIndex * 100) \/ itemsPerView;\n        sliderTrack.style.transform = `translateX(${resetTranslateX}%)`;\n      }, 500);\n    } else if (currentIndex <= 0) {\n      \/* If reached beginning of clones, reset to the original last item *\/\n      setTimeout(() => {\n        sliderTrack.style.transition = 'none';\n        currentIndex = totalItems - 1; \/\/ Set to last cloned item\n        const resetTranslateX = -(currentIndex * 100) \/ itemsPerView;\n        sliderTrack.style.transform = `translateX(${resetTranslateX}%)`;\n      }, 500);\n    }\n  }\n\n  \/* Function to start the automatic slide loop *\/\n  function startAutoSlide() {\n    autoSlideInterval = setInterval(() => {\n      currentIndex++;\n      updateSlider();\n    }, 4000); \/\/ Adjust interval as needed\n  }\n\n  \/* Function to reset the slide timer *\/\n  function resetAutoSlide() {\n    clearInterval(autoSlideInterval); \/\/ Clear the current interval\n    startAutoSlide(); \/\/ Restart the interval\n  }\n\n  \/* Next button functionality *\/\n  document.querySelector(\".arrow.right\").addEventListener(\"click\", function () {\n    currentIndex++;\n    updateSlider();\n    resetAutoSlide(); \/\/ Reset timer on button click\n  });\n\n  \/* Previous button functionality *\/\n  document.querySelector(\".arrow.left\").addEventListener(\"click\", function () {\n    currentIndex--;\n    updateSlider();\n    resetAutoSlide(); \/\/ Reset timer on button click\n  });\n\n  \/* Set initial position and start the automatic slide loop *\/\n  sliderTrack.style.transition = 'none';\n  const initialTranslateX = -(currentIndex * 100) \/ getItemsPerView();\n  sliderTrack.style.transform = `translateX(${initialTranslateX}%)`;\n  startAutoSlide(); \/\/ Start automatic sliding\n\n  \/* Update slider on window resize for responsiveness *\/\n  window.addEventListener(\"resize\", updateSlider);\n});\n\n\n\n<\/script>\n\n\n\n\n\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t","protected":false},"excerpt":{"rendered":"<p>We Don&#8217;t Just Build &#8211; We Define Culture Kitchen 1750+ Wardrobe 2300+ Living 1500+ Experience the World of Imly Where 